{
  "name": "offensive-vk",
  "version": "10.0.1",
  "description": "My Personal Github Repository. (Fully Automated)",
  "main": "README.md",
  "bot": "https://github.com/TheHamsterBot",
  "scripts": {
    "install": "pnpm add @octokit/rest @octokit/core @octokit/types octokit markdownlint",
    "source": "https://github.com/offensive-vk/offensive-vk",
    "dev": "sleep 5; echo 'Development Server Started... A Joke.' ",
    "image": "docker build -t vedansh-container .",
    "list": "docker ps -a",
    "pull": "docker pull ghcr.io/offensive-vk/offensive-vk:nightly",
    "compose": "docker compose up --build",
    "exec": "docker exec -it my-container neofetch",
    "spawn": "docker run -it -d --name my-container --rm -p 9999:9999 -v $(pwd):/shared/offensive-vk vedansh-container",
    "reset": "docker system prune -f"
  },
  "keywords": [
    "readme",
    "github",
    "profile",
    "daily",
    "automated",
    "yaml",
    "bot-managed"
  ],
  "author": {
    "name": "Vedansh âœ¨",
    "url": "https://github.com/offensive-vk/"
  },
  "license": "MIT",
  "packageManager": "pnpm@9.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/offensive-vk/"
  },
  "bugs": {
    "url": "https://github.com/offensive-vk/issues"
  },
  "homepage": "https://github.com/offensive-vk#readme",
  "dependencies": {
    "@octokit/core": "^6.1.2",
    "@octokit/rest": "^21.0.2",
    "@octokit/types": "^13.5.0",
    "markdownlint": "^0.35.0",
    "octokit": "^4.0.2"
  }
}
